{
  "expo": {
    "name": "BabySister",
    "slug": "chatapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/atm_card.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/atm_card.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.BabySister",
      "versionCode": 1,
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ]
    },
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#FF153E",
      "iconUrl": "https://d1wp6m56sqw74a.cloudfront.net/~assets/da4a0a2f789f1297c3e4a4bb4216d866",
      "androidMode": "default",
      "androidCollapsedTitle": ""
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Cho phép $(PRODUCT_NAME) sử dụng định vị của bạn."
        }
      ],
      [
        "expo-notifications",
        {
          "icon":  "./src/assets/atm_card.png"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "497e77fc-f310-435c-ab6a-5fd30b5c1550"
      }
    },
    "owner": "minhrxpo1505"
  }
}
